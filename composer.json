    "keywords": ["dotenv", "encryption", "env", "environment", "environment-variables", "environment-vars", "secure-env"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Uzair Hayat",
            "email": "business@uziiuzair.com",
            "homepage": "https://uziiuzair.com"
        }
